"const enolib = require('enolib');
const fs = require('fs');

exports.parse = path => {
  const input = fs.readFileSync(path, 'utf-8');
  const _document = enolib.parse(input, { source: path });

  const document = {};
  document.field = _document.field('field').requiredStringValue();
  {
    const _fieldset = _document.requiredFieldset('fieldset');
    document.fieldset = {};
    document.fieldset.entry = _fieldset.entry('entry').requiredStringValue();
  }
  document.list = _document.requiredList('list').requiredStringValues();
  {
    document.section = {};
    const _section = _document.requiredSection('section');
    const section = document.section;
    section.field = _section.field('field').requiredStringValue();
  }

  return document;
};"
